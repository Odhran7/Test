	<head>
		<title>Valumetrics App</title>

				<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
		<script>
		const clientStockData = {{{stockData}}}; // Handlebars will insert the stock data here
		window.onload = function() {
			const ctx = document.getElementById('stockChart').getContext('2d');
			new Chart(ctx, {
				type: 'line',
				data: {
					labels: clientStockData.map(data => data.date),
					datasets: [{
						label: ' Stock Price',
						data: clientStockData.map(data => data.price),
						borderColor: '#000',
						fill: false
					}]
				},
				options: {
					legend: {
						display: false,
					},
					scales: {
						x: {
							type: 'time',
							time: {
								parser: 'YYYY-MM-DD', // Specify the parser because the date format sent from the server is 'yyyy-mm-dd'
								tooltipFormat: 'll'
							},
							display: true,
							scaleLabel: {
								display: true,
								labelString: 'Date'
							}
						},
						y: {
							beginAtZero: true
						}
					}
				}
			});
		};
		</script>
		
